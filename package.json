{
  "name": "hokaze",
  "version": "0.4.0",
  "private": true,
  "workspaces": [
    "src/core"
  ],
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/types": "^7.9.0",
    "elzii-tools": "^0.2.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "prettier": "^3.6.2",
    "sync-json": "^1.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^7.9.0"
  },
  "sync": [
    "author",
    "homepage",
    "repository",
    "issues",
    "issues",
    "license",
    "type",
    "main",
    "module",
    "types",
    "files",
    "scripts"
  ],
  "author": "Louis Devie",
  "homepage": "https://github.com/louisdevie/hokaze",
  "repository": "https://github.com/louisdevie/hokaze",
  "issues": "https://github.com/louisdevie/hokaze/issues",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "sync-package-json": "sync-json -v -i sync --source ../../package.json package.json",
    "type-check": "tsc --noEmit",
    "lint": "eslint --config ../../eslint-typed.config.js index.ts src/** test/**",
    "test": "jest",
    "format": "prettier --write index.ts lib/** test/**",
    "build": "tsup"
  }
}
