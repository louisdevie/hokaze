{
  "name": "hokaze",
  "version": "0.4.0",
  "description": "Dayjs extension for Hokaze",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "sync-package-json": "sync-json -v -i sync --source package.json src/*/package.json"
  },
  "keywords": [],
  "author": "Louis DEVIE",
  "license": "MIT",
  "devDependencies": {
    "sync-json": "^1.0.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dayjs": "^1.11.10"
  },
  "peerDependencies": {
    "@hokaze/core": "^0.3.0"
  },
  "tsup": {
    "entry": [
      "./index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "minify": true
  },
  "private": true,
  "workspaces": [
    "src/core"
  ]
}